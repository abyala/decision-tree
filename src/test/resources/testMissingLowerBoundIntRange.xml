<decision-tree>
    <input-types>
        <integer-type name="range" min="unbounded" max="unbounded"/>
    </input-types>
    <result-type class="com.abyala.decisiontree.SimpleStringResult">
        <text-attribute name="value"/>
    </result-type>
    <tree>
        <range value="0"><result value="zero"/></range>
        <range value="1"><result value="positiveNumber"/></range>
        <range value="1000"><result value="largePositiveNumber"/></range>
    </tree>
</decision-tree>